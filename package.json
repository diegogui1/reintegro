{
  "name": "reitegrofactura",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^1",
    "@sap/cds": "^8",
    "@sap/cds-odata-v2-adapter-proxy": "^1.9.21",
    "@sap/xssec": "^4.2.7",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.7.0",
    "@cap-js/sqlite": "^1"
  },
  "scripts": {
    "start1": "cds watch ",
    "start": "cds-serve"
  },
  "cds": {
    "sql": {
      "native_hana_associations": false
    },
    "requires": {
     "[production]": {
        "auth": "dummy"
      },
      "destinations": true,
      "db": {
        "kind": "sqlite",
        "model": "db/schema.cds",
        "credentials": {
          "database": "my_facturas.db"
        }
      },
      "features": {
        "in_memory_db": false
      }
    }
  }
}
